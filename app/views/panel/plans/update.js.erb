$('div.admin-error').html('');
$('div.admin-error').css('display', 'none');

<% if @plan.errors.any? %>
  $('div.admin-error').html('<%= @plan.errors.full_messages.first %>');
  $('div.admin-error').css('display', 'block');

<% else %>
  $('#new-admin-modal').modal('hide');
  location.reload();
<% end %>
